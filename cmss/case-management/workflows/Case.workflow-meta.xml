    <rules>
        <fullName>ClientClaimRepeatedComplaintNotification</fullName>
        <actions>
            <name>ClientClaimRepeatedComplaintAlert</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>When Repeated Complaint is true or is changed, Alert is sent to Public Group Client Ombudsman</description>
        <formula>IsClientClaim__c = true
&amp;&amp;
(RepeatedComplaint__c = true ||PRIORVALUE(RepeatedComplaint__c))</formula>

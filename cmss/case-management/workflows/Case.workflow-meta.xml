    <rules>
        <fullName>ClientClaimInstitutionTypeNotification</fullName>
        <actions>
            <name>ClientClaimInstitutionTypeAlert</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>When Institution Type is true or is changed, Alert is sent to Public Group Client Ombudsman and Compliance manager</description>
        <formula>IsClientClaim__c = true &amp;&amp;	(
(ISNEW() &amp;&amp; NOT(ISBLANK(TEXT(InstitutionType__c)))) || 
(ISCHANGED(InstitutionType__c) &amp;&amp; NOT(ISNEW()))
				)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>ClientClaimNoAnswerTimeDependentNotification</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.Type</field>
            <operation>equals</operation>
            <value>1,2</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.ResponseSentDate__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>notEqual</operation>
            <value>2,3</value>
        </criteriaItems>
        <description>25 or 31 days after Received Date sends email alert to Client Claim case owner if the conditions are met.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <offsetFromField>Case.ReceivedDate__c</offsetFromField>
            <timeLength>25</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <offsetFromField>Case.ReceivedDate__c</offsetFromField>
            <timeLength>31</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>

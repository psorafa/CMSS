<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>reportNotProcessedInTimeFormula__c</fullName>
    <description>Is complaint and Days of complaint solving is more than 30 days.</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Type, &apos;1&apos;) &amp;&amp; DaysOfComplaintSolving__c &gt;30, 1, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Not Processed In Time Formula</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>

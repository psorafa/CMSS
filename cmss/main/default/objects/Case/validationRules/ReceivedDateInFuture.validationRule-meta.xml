<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ReceivedDateInFuture</fullName>
    <active>true</active>
    <description>Received Date can&apos;t be in the future.</description>
    <errorConditionFormula>$Profile.Name &lt;&gt; &apos;IntegrationUser&apos; &amp;&amp; 
(ReceivedDate__c &gt; TODAY())</errorConditionFormula>
    <errorDisplayField>ReceivedDate__c</errorDisplayField>
    <errorMessage>Received Date can&apos;t be in the future.</errorMessage>
</ValidationRule>
